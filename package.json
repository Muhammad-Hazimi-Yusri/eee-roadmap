{
  "name": "eee-roadmap",
  "type": "module",
  "version": "0.11.13",
  "description": "Interactive roadmap for Electrical & Electronic Engineering education",
  "scripts": {
    "download:pdfs": "node scripts/download-pdfs.mjs",
    "postinstall": "node scripts/setup-pdfjs.mjs",
    "dev": "npm run download:pdfs && astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check-links": "npx linkinator ./dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:all": "vitest run && playwright test"
  },
  "dependencies": {
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.16.0",
    "katex": "^0.16.27",
    "marked": "^17.0.1",
    "marked-katex-extension": "^5.1.6",
    "tailwindcss": "^3.4.0"
  },
  "keywords": [
    "electrical-engineering",
    "electronics",
    "education",
    "roadmap",
    "learning-path"
  ],
  "author": "Muhammad-Hazimi-Yusri",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "vitest": "^4.0.16"
  }
}
